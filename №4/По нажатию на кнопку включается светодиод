//Исходный код:
const int buttonPin = 34; // пин для внешней кнопки
const int ledPin = 13;   // встроенный светодиод
bool previousButtonState = HIGH;
bool ledState = false;
void setup() {
  pinMode(buttonPin, INPUT_PULLUP); // используем внутренний подтягивающий резистор
  pinMode(ledPin, OUTPUT);
  digitalWrite(ledPin, LOW);
  Serial.begin(9600);
}
void loop() {
  int buttonState = digitalRead(buttonPin);
  if (buttonState == LOW && previousButtonState == HIGH) {
    // при нажатии кнопки (от HIGH к LOW)
    ledState = !ledState; // переключить состояние светодиода
    digitalWrite(ledPin, ledState ? HIGH : LOW);
    delay(200); // задержка для устранения дребезга и стабильности
  }
  previousButtonState = buttonState;
}
