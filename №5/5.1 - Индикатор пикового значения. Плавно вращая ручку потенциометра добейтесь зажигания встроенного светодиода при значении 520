//Исходный код:
//ч.1
void setup() {
  Serial.begin(19200);
  pinMode (22, OUTPUT);
}
void loop() {
  Serial.println(analogRead(15));
  if (analogRead(15)>=520){
    digitalWrite (22, HIGH);
  }
  else{
    digitalWrite (22, LOW);
  }
}
//ч.2
void setup() {
  Serial.begin(19200);
  pinMode (22, OUTPUT);
}
void loop() {
  Serial.println(analogRead(15));
  if (analogRead(15)<=520){
    digitalWrite (22, HIGH);
  }
  else{
    digitalWrite (22, LOW);
  }
}
