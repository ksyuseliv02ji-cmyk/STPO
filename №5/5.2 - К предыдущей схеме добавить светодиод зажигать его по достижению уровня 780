//Исходный код:
void setup() {
  Serial.begin(19200);
  pinMode (22, OUTPUT);
  pinMode (23, OUTPUT);
}

void loop() {
  Serial.println(analogRead(15));
  if (analogRead(15)>=520){
    digitalWrite (22, HIGH);
  }
  else{
    digitalWrite (22, LOW);
  }
  if (analogRead(15)>=780){
    digitalWrite (23, HIGH);
  }
  else{
    digitalWrite (23, LOW);
  }
}
