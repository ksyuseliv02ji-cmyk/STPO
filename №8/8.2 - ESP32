//Исходный код для ESP32: 
#include <WiFi.h>
const char* apSSID = "ESP32_AP";
const char* apPassword = "12345678";
void setup() {
  Serial.begin(115200);
  delay(1000);
  Serial.println("Setting up access point...");
  WiFi.softAP(apSSID, apPassword);
  IPAddress IP = WiFi.softAPIP();
  Serial.print("AP IP address: ");
  Serial.println(IP);
}
